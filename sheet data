

// Assuming you have references to the source and target SpreadJS instances
var sourceSheet = sourceSpread.getSheet(0); // Assuming the source sheet is the first sheet
var targetSheet = targetSpread.getSheet(0); // Assuming the target sheet is the first sheet

// Define the source range (e.g., A1:B2)
var sourceRange = "A1:B2";

// Get the data, formulas, and cell tags from the source range
var sourceData = sourceSheet.getRange(sourceRange).getDataSource();
var sourceFormulas = sourceSheet.getRange(sourceRange).formulas();
var sourceCellTags = sourceSheet.getRange(sourceRange).tag();

// Define the target range where you want to copy the data
var targetRange = "C3"; // Adjust as needed

// Set the data, formulas, and cell tags to the target range
targetSheet.getRange(targetRange).setDataSource(sourceData);
targetSheet.getRange(targetRange).formulas(sourceFormulas);
targetSheet.getRange(targetRange).tag(sourceCellTags);

// Optionally, save the target file
