!!!!! Load the two Excel files
var workbook1 = new GC.Spread.Sheets.Workbook(document.getElementById('workbook1'));
var workbook2 = new GC.Spread.Sheets.Workbook(document.getElementById('workbook2'));

// Get the sheets you want to compare
var sheet1 = workbook1.getSheet(0);
var sheet2 = workbook2.getSheet(0);

// Iterate through cells and compare values
for (var i = 0; i < sheet1.getRowCount(); i++) {
    for (var j = 0; j < sheet1.getColumnCount(); j++) {
        var value1 = sheet1.getValue(i, j);
        var value2 = sheet2.getValue(i, j);
        
        // Compare cell values
        if (value1 !== value2) {
            // Highlight or mark the differing cells
            sheet1.getCell(i, j).backColor("red");
            sheet2.getCell(i, j).backColor("red");
        }
    }
}


!!

// Assuming you have two ranges: range1 and range2

var range1 = sheet.getRange(0, 0, 5, 5); // Example range 1
var range2 = sheet.getRange(0, 0, 5, 5); // Example range 2

var isEqual = true;

range1.forEach(function(row, rowIndex) {
    row.forEach(function(cell, colIndex) {
        if(cell.value() !== range2.getValue(rowIndex, colIndex)) {
            isEqual = false;
            return false; // Exit loop early if a difference is found
        }
    });
});

if(isEqual) {
    console.log("The ranges are equal.");
} else {
    console.log("The ranges are not equal.");
}


